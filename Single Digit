n=int(input())
if(n==0):
  print("zero")
elif(n==1):
  print("one")
elif(n==2):
  print("Two")
elif(n==3):
  print("Three")
elif(n==4):
  print("Four")
elif(n==5):
  print("Five")
elif(n==6):
  print("Six")
elif(n==7):
  print("Seven")
elif(n==8):
  print("Eight")
elif(n==9):
  print("Nine")
else:
  print("Given number is not a single number")
